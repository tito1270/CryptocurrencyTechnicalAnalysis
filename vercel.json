{
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/kucoin/(.*)",
      "destination": "https://api.kucoin.com/$1"
    }
  ],
  "functions": {
    "app/api/contact/route.ts": {
      "maxDuration": 10
    }
  }
}
